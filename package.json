    "test:ci": "npm run test --ci"
  },
  "dependencies": {
    "@strapi/design-system": "^1.6.3",
    "@strapi/helper-plugin": "^4.6.0",
    "@strapi/icons": "^1.6.3",
    "@strapi/strapi": "^4.21.0",
    "@strapi/utils": "^4.21.0",
    "algoliasearch": "^4.22.1",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@strapi/typescript-utils": "^4.6.0",
    "@types/jest": "^29.5.8",
    "@types/react": "^17.0.53",
