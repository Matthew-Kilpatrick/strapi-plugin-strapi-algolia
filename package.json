{
  "name": "strapi-plugin-strapi-algolia",
  "version": "1.0.0",
  "description": "Index articles configured to Algolia",
  "keywords": [
    "strapi",
    "plugin",
    "algolia"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wizbii/strapi-algolia.git"
  },
  "license": "MIT",
  "author": {
    "name": "Wizbii"
  },
  "maintainers": [
    {
      "name": "Wizbii"
    }
  ],
  "files": [
    "dist/admin/**/*.js",
    "dist/admin/**/*.json",
    "dist/server/**/*.js",
    "dist/utils/**/*.js",
    "strapi-admin.js",
    "strapi-server.js"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.server.json",
    "develop": "tsc -p tsconfig.server.json -w",
